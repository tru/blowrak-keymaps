// whatever


// Blowrak for swedish
partial default alphanumeric_keys 
xkb_symbols "blowrak" {

    name[Group1]="Blowrak - swedish";

    include "se(basic)"

    key <AD01> { [     aring,      Aring,    braceleft,  bracketleft ]	};
    key <AD02> { [  adiaeresis,	Adiaeresis,        bar,    backslash ]	};
    key <AD03> { [ odiaeresis, Odiaeresis,  braceright, bracketright ]	};
    key <AD04> { [         p,          P,   asciitilde,        THORN ]	};
    key <AD05> { [         y,          Y,    leftarrow,          yen ]	};
    key <AD06> { [         f,          F,      dstroke,  ordfeminine ]	};
    key <AD07> { [         g,          G,          eng,          ENG ]	};
    key <AD08> { [         c,          C,    copyright,    copyright ]	};
    key <AD09> { [         r,          R,   registered,   registered ]	};
    key <AD10> { [         l,          L,      lstroke,      Lstroke ]	};
    key <AD11> { [     comma,  semicolon, dead_cedilla,  dead_ogonek ]	};
    key <AD12> { [ dead_diaeresis, dead_circumflex, dead_tilde,	asciicircum	]	};

    key <AE04> { [         4,     dollar,      currency,         cent ] };

    key <AC01> { [         a,          A,   bracketleft,           AE ]	};
    key <AC02> { [         o,          O,     backslash,           OE ]	};
    key <AC03> { [         e,          E,  bracketright,         cent ]	};
    key <AC04> { [         u,          U,   asciicircum,      uparrow ]	};
    key <AC05> { [         i,          I,   rightarrow,     idotless ]	};
    key <AC06> { [         h,          H,      hstroke,      Hstroke ]	};
    key <AC07> { [         d,          D,          eth,          ETH ]	};
    key <AC08> { [         t,          T,       tslash,       Tslash ]	};
    key <AC09> { [         n,          N ]	};
    key <AC10> { [         s,          S,       ssharp,      section ]	};
    key <AC11> { [     minus, underscore, dead_belowdot, dead_abovedot ] };

    key <AB01> { [    period,   colon, periodcentered, dead_abovedot ]	};
    key <AB02> { [         q,          Q,           at,  Greek_OMEGA ]	};
    key <AB03> { [         j,          J ]	};
    key <AB04> { [         k,          K,          kra,    ampersand ]	};
    key <AB05> { [         b,          B, rightdoublequotemark, apostrophe ] };
    key <AB06> { [         x,          X, guillemotright,    greater ]	};
    key <AB07> { [         m,          M,           mu,    masculine ]	};
    key <AB08> { [         w,          W,      lstroke,      Lstroke ]	};
    key <AB09> { [         v,          V, leftdoublequotemark, grave ]	};
    key <AB10> { [         z,          Z, guillemotleft,        less ]	};
};



// Blowrak for swedish
partial alphanumeric_keys
xkb_symbols "blowrakteck" {

    name[Group1]="Blowrak on TECK";

    include "latin(type2)"


// copy paste from above - should be fixed to a generic include.

    key <AD01> { [     aring,      Aring,    braceleft,  bracketleft ]	};
    key <AD02> { [  adiaeresis,	Adiaeresis,        bar,    backslash ]	};
    key <AD03> { [ odiaeresis, Odiaeresis,  braceright, bracketright ]	};
    key <AD04> { [         p,          P,   asciitilde,        THORN ]	};
    key <AD05> { [         y,          Y,    leftarrow,          yen ]	};
    key <AD06> { [         f,          F,      dstroke,  ordfeminine ]	};
    key <AD07> { [         g,          G,          eng,          ENG ]	};
    key <AD08> { [         c,          C,    copyright,    copyright ]	};
    key <AD09> { [         r,          R,   registered,   registered ]	};
    key <AD10> { [         l,          L,      lstroke,      Lstroke ]	};
    key <AD11> { [     comma,  semicolon, dead_cedilla,  dead_ogonek ]	};
    key <AD12> { [ dead_diaeresis, dead_circumflex, dead_tilde,	asciicircum	]	};

    key <AC01> { [         a,          A,   bracketleft,           AE ]	};
    key <AC02> { [         o,          O,     backslash,           OE ]	};
    key <AC03> { [         e,          E,  bracketright,         cent ]	};
    key <AC04> { [         u,          U,   asciicircum,      uparrow ]	};
    key <AC05> { [         i,          I,   rightarrow,     idotless ]	};
    key <AC06> { [         h,          H,      hstroke,      Hstroke ]	};
    key <AC07> { [         d,          D,          eth,          ETH ]	};
    key <AC08> { [         t,          T,       tslash,       Tslash ]	};
    key <AC09> { [         n,          N ]	};
    key <AC10> { [         s,          S,       ssharp,      section ]	};
    key <AC11> { [     minus, underscore, dead_belowdot, dead_abovedot ] };

    key <AB01> { [    period,   colon, periodcentered, dead_abovedot ]	};
    key <AB02> { [         q,          Q,           at,  Greek_OMEGA ]	};
    key <AB03> { [         j,          J ]	};
    key <AB04> { [         k,          K,          kra,    ampersand ]	};
    key <AB05> { [         b,          B, rightdoublequotemark, apostrophe ] };
    key <AB06> { [         x,          X, guillemotright,    greater ]	};
    key <AB07> { [         m,          M,           mu,    masculine ]	};
    key <AB08> { [         w,          W,      lstroke,      Lstroke ]	};
    key <AB09> { [         v,          V, leftdoublequotemark, grave ]	};
    key <AB10> { [         z,          Z, guillemotleft,        less ]	};


// teck specific
    key <AE11>	{ [     plus, question,    questiondown, backslash ]	};
    key <AE12>	{ [     EuroSign,       plus, dead_cedilla,  dead_ogonek ]	};

    key <AD12> { [     minus, underscore, dead_belowdot, dead_abovedot ] };
    key <AC11> { [         z,          Z, guillemotleft,        less ]	};
    key <AB10> { [      apostrophe,        asterisk,           acute,        multiply ] };

    key <BKSL> { [      less,     greater,         bar,    brokenbar ] };

// swap control and shift
    key <LFSH> { [ Control_L  ] };
    key <LCTL> { [ Shift_L  ] };
    key <RTSH> { [ Control_R  ] };
    key <RCTL> { [ Shift_R  ] };

    key <X000> { [ Alt_L  ] };
    //key <LALT> { [ Super_R  ] };

//  modifier_map Mod1   { <X000>, <RALT> };
//  modifier_map Mod5   { <LALT>, <LVL3> };

};
